@model Bar.Models.Beer
@{
    ViewData["Title"] = "@Model.Name";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@Model.Name</h1>
<hr />
@foreach(Bar.Models.BeerPatron beerPatron in Model.BeerPatrons.ToList())
{
    <div class="card">
        <div class="card-content">
            <h4><a href="/Beers/Details/">@beerPatron.Patron.Name</a></h4>
        </div>
    </div>
}